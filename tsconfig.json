{
  "extends": "./tsconfig.base.json",
  "files": [],
  "typedocOptions": {
    "entryPointStrategy": "packages",
    "entryPoints": [
      "pkgs/core",
      "pkgs/propagators/b3",
      "pkgs/resources",
      "pkgs/semantic-conventions"
    ],
    "out": "docs",
